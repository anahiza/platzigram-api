{
  "name": "platzigram-api",
  "version": "1.0.0",
  "description": "Platzigram REST API",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && NODE_ENV=test ava test/**-test.js"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "ava": {
    "require": ["babel-register"]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "author": "Anahi Zentella Aguirre",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-eslint": "^7.1.1",
    "babel-register": "^6.18.0",
    "standard": "^8.6.0",
    "test-listen": "^1.0.0"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "http-hash": "^2.0.0",
    "request": "^2.79.0"
  }
}
